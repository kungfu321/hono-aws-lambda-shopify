{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-expressions": "warn",
    "no-unused-labels": "warn",
    "no-unused-imports": "warn",
    "no-unused-imports-ts": "warn",
    "no-unused-imports-tsx": "warn"
  }
}